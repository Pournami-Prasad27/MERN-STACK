filename=shift.js

IMPORT

function greet(name){
    return `Hello,${name}!`;
}

module.exports ={greet,add:(a,b) => a+b,subtract:(a,b) => a-b}// used for function call with different functions

EXPORT

const myModule= require('./Shift');

console.log(myModule.greet('pournami'));

console.log(myModule.add(5,3));
console.log(myModule.subtract(5,3));
